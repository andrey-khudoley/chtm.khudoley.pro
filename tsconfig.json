{
  "compilerOptions": {
    "target": "es2021",
    "module": "es2020",
    "moduleResolution": "node",
    "lib": ["es2021", "dom"],
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      
      "/*": ["./*"],
      "@andrey-khudoley/*": ["./*"]
    },
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "jsxFactory": "jsx",
    "jsxFragmentFactory": "jsx.Fragment",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "types": ["vue", "./_stage/neso-assistant/web/v1/types/vue-modules.d.ts"]
  },
  "vueCompilerOptions": {
    "globalTypesPath": "./_stage/neso-assistant/web/v1/types/vue.d.ts",
    "target": 3,
    "strictTemplates": false,
    "skipTemplateCodegen": false,
    "nativeTags": ["template", "script", "style"]
  },
  "include": [
    "**/*", ".**/*"
, "_stage/neso-assistant/index.tsx"  ],
  "exclude": [
    "./node_modules/**/*"
  ],
  "files": [
    "./node_modules/@app/types/globals.d.ts",
    "./node_modules/@app/types/modules.d.ts",
    "./node_modules/@app/ui/global-jsx.d.ts",
    "./_stage/neso-assistant/web/v1/types/vue.d.ts",
    "./_stage/neso-assistant/web/v1/types/vue-modules.d.ts"
],
  "references": [
    
  ]
}
